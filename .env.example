# Gemini API Configuration (used for abstracts and summary)
Gemini_API_KEY=your_api_key
Gemini_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
# Models
# Article → Abstract step
Gemini_ABSTRACT_MODEL_ID=gemini-2.5-flash
# Abstract → Summary step
Gemini_SUMMARY_MODEL_ID=gemini-2.5-pro
# Optional legacy key for fallback if the above are unset
# Gemini_MODEL_ID=

# Performance tuning
# Control concurrency for abstract generation (default 20). Lower if provider throttles.
# ABSTRACT_MAX_WORKERS=3

# Database Location
DB_PATH=your_freshrss_db_path

# Feed Sources
ALLOWED_FEED_NAMES="feed_name_1,feed_name_2,feed_name_3"
WECHAT_CATEGORY_ID="category_id"
WECHAT_URL_PATTERN_CONTAINS="wechat2rss_or_other_services"

# Dropbox Config
DROPBOX_APP_KEY=your_app_key
DROPBOX_APP_SECRET=your_app_secret
DROPBOX_REFRESH_TOKEN=your_refresh_token
